{

    "description": "Improve AA PBS search",
    "manifest_version": 2,
    "name": "pbs-hack",
    "version": "1.0",
  
    "permissions": [
      "webRequest", "webRequestBlocking", "https://fapbs.aa.com/*"
    ],
  
    "background": {
      "scripts": ["background.js"]
    },
  
    "browser_specific_settings": {
      "gecko": {
        "strict_min_version": "57.0a1"
      }
    }
  
  }